{
  "manifest_version": 3,
  "name": "ZV Downloader",
  "version": "1.0",
  "description": "Herramienta de descarga creada por https://ZonaVirtual.Club, dise√±ada y construida por Centurion Carlos Ezequiel",
  "permissions": [
    "storage",
    "activeTab",
    "windows",
    "scripting",
    "downloads",
    "tabs"
  ],
  "host_permissions": [
    "https://zona-virtual-cloud-backend.carlos-mdtz9.workers.dev/"
